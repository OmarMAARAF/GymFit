<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Layout.muscleActivity">
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_arrow_back_ios_24"
        android:background="@drawable/image_button_border"
        android:id="@+id/backBtntohome"
        android:paddingVertical="10dp"
        android:paddingLeft="15dp"
        android:paddingRight="7dp"
        android:layout_margin="20dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bb"
        android:layout_toRightOf="@id/backBtntohome"
        android:textAlignment="center"
        android:layout_marginTop="29dp"
        android:textSize="17dp"
        android:textColor="@color/black"
        android:text="chest workout"
        android:fontFamily="@font/poppins_regular"
        />


    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/chestListView"
        android:layout_marginTop="80dp"
        />

</RelativeLayout>